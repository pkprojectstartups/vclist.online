---
layout: none
permalink: /sitemap/sitemap-category-pagination.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% if page.url contains 'custom_lists/' and page.url contains '/page' %}
      {% assign parts = page.url | split: '/' %}
      {% assign filename = parts.last %}
      {% assign page_num = filename | remove: 'page' | remove: '.html' | plus: 0 %}
      {% if page_num <= 3 %}
        <url>
          <loc>{{ page.url | absolute_url }}</loc>
        </url>
      {% endif %}
    {% endif %}
  {% endfor %}
</urlset>
